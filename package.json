{
  "name": "json2typescripts",
  "displayName": "JSON to TypeScript",
  "description": "This is an extension for Visual Studio Code that allows you to convert json to ts",
  "version": "1.0.0",
  "author": {
    "name": "GDYG",
    "email": "gdyg01@gmail.com"
  },
  "keywords": [
    "json",
    "typescript",
    "json-to-ts",
    "json2ts",
    "json2typescripts",
    "json to ts",
    "json to typescript",
    "vscode"
  ],
  "publisher": "Ann",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GDYG/json-to-ts"
  },
  "homepage": "https://github.com/GDYG/json-to-ts?tab=readme-ov-file#readme",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "icon": "public/assets/images/json2ts.png",
  "contributes": {
    "commands": [
      {
        "command": "json2typescripts.convertJsonToTs",
        "title": "Convert JSON to TypeScript"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "json2typescripts.convertJsonToTs",
          "when": "editorTextFocus && !editorReadonly",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "json2typescripts.convertJsonToTs",
        "key": "ctrl+alt+j",
        "when": "editorTextFocus",
        "mac": "cmd+alt+j"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "sinon": "^18.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "json-to-ts": "^2.1.0"
  }
}
